
<%= simple_form_for [@spot, @review] do |f| %>
<%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :description, label: "Describe your spot"  %>
    <%= f.label :name, "Rate your spot"   %>
     <%= f.select :rating, [*1..10] %>
     <br>
    <%= f.submit "Create spot review", class: "btn btn-primary" %>
  </div>
<% end %>




